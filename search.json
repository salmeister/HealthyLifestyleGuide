---
layout: null
---
[
  {% for note in site.notes %}
  {% unless forloop.first %},{% endunless %}
  {
    "url": "{{ note.url | relative_url }}",
    "content": {{ note.content | strip_html | strip_newlines | normalize_whitespace | jsonify }},
    "title": {{ note.title | jsonify }},
    "description": "{% if note.description %}{{ note.description }}{% elsif note.excerpt %}{{ note.excerpt | strip_html | strip_newlines | truncate: 160 }}{% endif %}",
    "category": "{{ note.category }}",
    "author": "{{ note.author }}",
    "keywords": "{{ note.keywords }}",
    "date": "{% if note.date %}{{ note.date | date: '%B %d, %Y' }}{% endif %}"
  }
  {% endfor %}
]