---
layout: none
---
[
  {% assign files = site.static_files | where_exp: "file", "file.path contains '/_notes/'" | where_exp: "file", "file.extname == '.md'" %}
  {% for file in files %}
    {% capture content %}{% include_relative {{ file.path }} %}{% endcapture %}
    {
      "title": "{{ file.path | split: '/' | last | remove: '.md' | replace: '-', ' ' }}",
      "url": "{{ file.path | relative_url }}",
      "content": {{ content | jsonify }}
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]