---
layout: none
---
[
  {% assign notes = site.pages %}
  {% for note in notes %}
    {% if note.path contains '_notes/' %}
    {
      "title": "{{ note.path | split: '/' | last | remove: '.md' | replace: '-', ' ' }}",
      "url": "{{ note.url | relative_url }}",
      "content": {{ note.content | strip_html | strip_newlines | jsonify }}
    }{% unless forloop.last %},{% endunless %}
    {% endif %}
  {% endfor %}
]