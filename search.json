---
layout: null
---
[
  {
    "title": "Home",
    "url": "{{ '/' | relative_url }}",
    "content": {{ site.pages | where: "name", "index.md" | map: "content" | first | strip_html | strip_newlines | normalize_whitespace | jsonify }}
  }{% if site.pages.size > 0 %},{% endif %}
  
  {% assign notes_pages = site.pages | where_exp: "page", "page.path contains '_notes/'" %}
  {% for page in notes_pages %}
    {
      "title": "{{ page.path | split: '/' | last | remove: '.md' | replace: '-', ' ' }}",
      "url": "{{ page.url | relative_url }}",
      "category": "{{ page.path | split: '/' | slice: 1 | first }}",
      "content": {{ page.content | strip_html | strip_newlines | normalize_whitespace | jsonify }}
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]