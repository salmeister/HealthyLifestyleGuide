---
layout: null
---
[
  {
    "title": "Home",
    "url": "/",
    "category": "home",
    "content": {{ site.pages | where: "name", "index.md" | map: "content" | first | strip_html | strip_newlines | normalize_whitespace | jsonify }}
  }
  
  {% for note in site.notes %}
  ,{
    "url": "{{ note.url | relative_url }}",
    "category": "{{ note.path | split: '/' | slice: 2 | first }}",
    "content": {{ note.content | strip_html | strip_newlines | normalize_whitespace | jsonify }},
    "displayTitle": "{{ note.content | split: '## [⬅️](/)' | last | split: '##' | first | strip_html | strip_newlines | normalize_whitespace | jsonify }}"
  }
  {% endfor %}
]